{
  "name": "aurora",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "node",
  "scripts": {
    "build": "tsup src/ --format esm --out-dir dist --clean",
    "start": "node ./dist/bin/www.js",
    "dev": "nodemon --watch src --exec node --loader ts-node/esm ./src/bin/www.ts",
    "serve": "npm run build && node ./dist/bin/www.js",
    "lint": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:prisma": "node --loader ts-node/esm src/test-prisma.ts"
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.5.1",
    "@prisma/client": "^6.19.0",
    "@prisma/extension-optimize": "^2.0.0",
    "@prisma/instrumentation": "^6.18.0",
    "@supabase/supabase-js": "^2.76.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-jwt": "^8.5.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.3",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "pug": "^3.0.3",
    "supabase": "^2.53.6",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/express-jwt": "^6.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.0",
    "@types/pg": "^8.15.5",
    "nodemon": "^3.1.10",
    "prisma": "^6.19.0",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
